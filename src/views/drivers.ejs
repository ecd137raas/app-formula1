<!DOCTYPE html>
<html lang="en">

<head>
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0/dist/css/bootstrap.min.css" rel="stylesheet"
    integrity="sha384-gH2yIJqKdNHPEq0n4Mqa/HGKIhSkIHeL5AyhkYV8i59U5AR6csBvApHHNl/vI1Bx" crossorigin="anonymous">
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Results F1 | Drivers</title>
</head>

<body>
  <header>
    <nav class="navbar navbar-expand-lg bg-light">
      <div class="container-fluid">
        <a class="navbar-brand" href="/">
          <img
            src="data:image/png;base64,"
            alt="" width="30" height="24">
        </a>
        <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarNav"
          aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
          <span class="navbar-toggler-icon"></span>
        </button>
        <div class="collapse navbar-collapse" id="navbarNav">
          <ul class="navbar-nav">
            <li class="nav-item">
              <a class="nav-link active" aria-current="page" href="/">Home</a>
            </li>
            <li class="nav-item">
              <a class="nav-link" href="/results">Last Race</a>
            </li>
            <li class="nav-item">
              <a class="nav-link" href="/driverstandings">Drivers</a>
            </li>
            <li class="nav-item">
              <a class="nav-link" href="/constructors">Constructor</a>
            </li>
          </ul>
        </div>
      </div>
    </nav>
  </header>
  <main style="margin-top: 5px;">
    <div class="container" style="display:flexbox">
      <div class="row justify-content-center">
        <div class="col">
          <div class="card h-100">
            <div class="card-body">
              <h3 class="display-6">Drivers</h3>
              <h5>2022 Drivers Standings</h5>
            </div>
          </div>
        </div>
      </div>
      <p></p>
      <% const driverstan=res.MRData.StandingsTable.StandingsLists %>
        <% driverstan.forEach(function(driverstan){ %>
          <div class=" row justify-content-center">
            <div class="container-fluid">
              <div class="card">
                <div class="card-body">
                  <table class="table table-hover">
                    <tr class="table-secondary">
                      <th>Position</th>
                      <th>Points</th>
                      <th>Wins</th>
                      <th>Driver</th>
                    </tr>

                    <% for(var i=0; i < driverstan.DriverStandings.length; i++){%>
                      <tr>
                        <td>
                          <%=driverstan.DriverStandings[i].position%>
                        </td>
                        <td>
                          <%=driverstan.DriverStandings[i].points%>
                        </td>
                        <td>
                          <%=driverstan.DriverStandings[i].wins%>
                        </td>
                        <td>
                          <%=driverstan.DriverStandings[i].Driver.givenName%>
                            <%=driverstan.DriverStandings[i].Driver.familyName%>
                        </td>

                      </tr>
                      <%}%>
                  </table>
                </div>
              </div>
            </div>
          </div>
          <%});%>
    </div>
  </main>
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0/dist/js/bootstrap.bundle.min.js"
    integrity="sha384-A3rJD856KowSb7dwlZdYEkO39Gagi7vIsF0jrRAoQmDKKtQBHUuLZ9AsSv4jD4Xa"
    crossorigin="anonymous"></script>
</body>

</html>